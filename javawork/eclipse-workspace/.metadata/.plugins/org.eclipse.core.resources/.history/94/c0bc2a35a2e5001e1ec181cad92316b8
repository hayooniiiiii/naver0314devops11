package day0319;

public class While {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=10;
		while(n>10) {
			System.out.println("Hello #1");
		}
		System.out.println();
		do {
			System.out.println("Hello #2");
			
		}while(n>10);
		
		int a=1;
		while(a<10) {
			System.out.printf("%3d", a++);
		}

	}

}
