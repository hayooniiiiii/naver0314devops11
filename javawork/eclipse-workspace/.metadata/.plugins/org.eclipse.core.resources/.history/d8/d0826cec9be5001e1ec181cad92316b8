package day0319;

public class Ex2_Munje {

	public static void main(String[] args) {
		int sum=0;
		// TODO Auto-generated method stub
		loop:
			for(int i=1;i<15;i++) {
				for(int j=1;j<=i;j++) {
					sum=sum+j;
					System.out.printf("%d ",sum);
					if(i==10) {
						break loop;
					}
				}
				System.out.println();
			}
		

	}

}
